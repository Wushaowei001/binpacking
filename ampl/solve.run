option presolve 0;

option solver cplexamp;

problem MinUB: x, cUB, Assign, Dimensions;
problem MaxLB: x, cLB, Assign, Dimensions;

param tmp default 0;
for {t in K} {
   let r := t; 

   solve MinUB; 
   let tmp := cUB;
  
   problem MaxLB;
   solve MaxLB;
   printf "%d#[%.2f,%d]\n", b[r], cLB, tmp;
}
